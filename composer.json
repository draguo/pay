{
    "name": "draguo/pay",
    "description": "from yansongda/pay",
    "keywords": ["alipay", "wechat", "pay"],
    "type": "library",
    "require": {
        "php": ">=7.1.3",
        "ext-openssl": "*",
        "ext-simplexml":"*",
        "ext-libxml": "*",
        "ext-json": "*",
        "yansongda/supports": "^2.0",
        "symfony/http-foundation": "^4.0|^5.0.7",
        "symfony/event-dispatcher": "^4.0|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Draguo\\Pay\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Draguo\\Pay\\Tests\\": "tests"
        }
    },
    "license": "MIT"
}
