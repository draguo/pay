{
    "name": "draguo/pay",
    "description": "from yansongda/pay",
    "keywords": ["alipay", "wechat", "pay"],
    "type": "library",
    "require": {
        "php": ">=7.0",
        "ext-openssl": "*",
        "ext-simplexml":"*",
        "ext-libxml": "*",
        "ext-json": "*",
        "yansongda/supports": "^1.8",
        "monolog/monolog": "^1.23",
        "symfony/http-foundation": "^3.0|^4.0",
        "symfony/event-dispatcher": "^3.0|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Draguo\\Pay\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Draguo\\Pay\\Tests\\": "tests"
        }
    },
    "license": "MIT"
}
